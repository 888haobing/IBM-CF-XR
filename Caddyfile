localhost:8080

root * /usr/local/bin
file_server browse
tls love@xray.org
tls_client_auth /usr/local/bin/cert.crt /usr/local/bin/cert.key

@websockets_XL_VM {
     header Connection Upgrade
     header Upgrade websocket
     path /${XR_VM_UUID}-vmess
}
reverse_proxy @websockets_XL_VM localhost:8079


@websockets_XL_VL {
     header Connection Upgrade
     header Upgrade websocket
     path /${XR_VL_UUID}-vless
}
reverse_proxy @websockets_XL_VL localhost:8080
