@websocket_XR_VM {
     header Connection Upgrade
     header Upgrade websocket
     path /${XR_VM_UUID}-vmess
}
reverse_proxy @websocket_XR_VM localhost:8080

@websocket_XR_VL {
     header Connection Upgrade
     header Upgrade websocket
     path /${XR_VL_UUID}-vless
}
reverse_proxy @websocket_XR_VL localhost:8080
